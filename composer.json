{
    "name": "slvler/phone-validation",
    "license": "MIT",
    "description": "Phone Validation API for app.abstractapi.com",
    "type": "package",
    "authors": [
        {
            "name": "slvler",
            "email": "slvler@proton.me"
        }
    ],
    "autoload": {
