    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "slvler\\PhoneValidation\\PhoneValidationServiceProvider"
            ],
            "aliases": {
                "PhoneValidation": "slvler\\PhoneValidation\\Facades\\PhoneValidation"
            }
        }
    }
